package com.bridgelabz.logicalProgramming;

public class Gambler {
	
	public static void main(String[] args) {
		
	}
	
	public static 

}

public class ResultGambler{
	private int win;
	private float winPercentage;
	private float lossPercentage;
	public ResultGambler(int win, float winPercentage, float lossPercentage) {
		super();
		this.win = win;
		this.winPercentage = winPercentage;
		this.lossPercentage = lossPercentage;
	}
	public int getWin() {
		return win;
	}
	public void setWin(int win) {
		this.win = win;
	}
	public float getWinPercentage() {
		return winPercentage;
	}
	public void setWinPercentage(float winPercentage) {
		this.winPercentage = winPercentage;
	}
	public float getLossPercentage() {
		return lossPercentage;
	}
	public void setLossPercentage(float lossPercentage) {
		this.lossPercentage = lossPercentage;
	}
	
	
}
